{
  "name": "media-core-ui",
  "version": "0.1.0",
  "description": "Media core UI",
  "repository": {
    "type": "git",
    "url": "https://github.com/reachlocal/media-core-ui"
  },
  "scripts": {
    "lint": "eslint ./src/app/**/* ./test/app/**/* --ext=js,json; stylelint ./src/app/**/*.scss; exit 0;",
    "start": "webpack-dev-server --config ./webpack/development.config.js --hot",
    "start:test": "webpack-dev-server --config ./webpack/test.config.js --hot",
    "pretest:e2e": "webdriver-manager update",
    "test": "npm run lint; npm run test:unit:single-run; npm run pretest:e2e; npm run test:e2e:headless;",
    "test:e2e": "npm run start:test & protractor test/protractor.conf.js; exit 0;",
    "test:e2e:headless": "npm run start:test & protractor test/headless-protractor.conf.js; exit 0;",
    "test:unit": "karma start ./karma.conf.js; exit 0;",
    "test:unit:single-run": "karma start ./karma.conf.js --single-run; exit 0;",
    "build": "webpack --config ./webpack/production.config.js",
    "build:bamboo": "npm install;  npm run test; npm run build;"
  },
  "dependencies": {
    "angular": "^1.6.4",
    "angular-animate": "^1.6.4",
    "angular-cookies": "^1.6.4",
    "angular-http-auth": "^1.5.0",
    "angular-jwt": "^0.1.9",
    "angular-sanitize": "^1.6.4",
    "angular-translate": "^2.15.1",
    "angular-ui-bootstrap": "^2.5.0",
    "angular-ui-router": "1.0.0-rc.1",
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "bootstrap-sass": "^3.3.7",
    "chart.js": "^2.5.0",
    "clean-webpack-plugin": "^0.1.10",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.4.3",
    "ng-annotate-loader": "^0.2.0",
    "node-sass": "^4.5.0",
    "sass-loader": "^6.0.3",
    "select2": "^3.4.5",
    "style-loader": "^ 0.14.0",
    "ui-select": "^0.19.8",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.8",
    "eslint": "^3.18.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-json": "^1.2.0",
    "jasmine": "^2.5.3",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.3",
    "protractor": "^5.1.1",
    "stylelint": "^7.9.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "webpack-dev-server": "^2.4.2"
  }
}
